{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "script"
  },
  "plugins": ["@typescript-eslint", "unicorn"],
  "rules": {
    "unicorn/numeric-separators-style": ["error", {"number": {"minimumDigits": 7, "groupLength": 3}}]
  },
  "extends": ["plugin:@typescript-eslint/recommended"]
}
